        {"enableBrakeThermals":true},
        {"brakeDiameter":0.185},
        {"brakeMass":"$=7.2*$brakeThickness_F"},
        {"brakeType":"vented-disc"},
        {"rotorMaterial":"steel"},
        {"padMaterial":"semi-race", "padGlazingSusceptibility":0.1},
        {"brakeVentingCoef":"$=$components.brakes.brakeVentingCoef_F"},
        //sound
        {"squealCoefNatural": 0.9, "squealCoefLowSpeed": 1.0}
    ]
