    ],
    "flexbodies": [
        ["mesh", "[group]:", "nonFlexMaterials"],
        ["brake_caliper_4pot_red", ["Astral_hub_FR", "axf_R"], [], {"pos":{"x":-0.79, "y":-0.015, "z":0}, "scale":{"x": 1,  "y":0.95,  "z":0.95}, "rot":{"x":-15, "y":0, "z":0}}], 
        ["brake_caliper_4pot_red", ["Astral_hub_FL", "axf_L"], [], {"pos":{"x": 0.79, "y":-0.015, "z":0}, "scale":{"x": 1,  "y":0.95,  "z":0.95}, "rot":{"x":-15, "y":0, "z":0}}], 
        ["brake_hub_5l",                   ["wheel_FR",     "axf_R"], [], {"pos":{"x":-0.765, "y":0, "z":0}, "scale":{"x":1.05,  "y":1.0,  "z":1.0},  "rot":{"x":0,  "y":0,  "z":0}}],
        ["brake_hub_5l",                   ["wheel_FL",     "axf_L"], [], {"pos":{"x": 0.765, "y":0, "z":0}, "scale":{"x":1.05,  "y":1.0,  "z":1.0},  "rot":{"x":0,  "y":0,  "z":180}}],
        ["brake_disc_slotted",             ["wheel_FR",     "axf_R"], [], {"pos":{"x":-0.79, "y":0, "z":0}, "scale":{"x":1.21, "y":0.92, "z":0.92}, "rot":{"x":0,  "y":0,  "z":0}, "materialOverride":[["disc_brake", "disc_brake_FR"]]}],
        ["brake_disc_slotted",             ["wheel_FL",     "axf_L"], [], {"pos":{"x": 0.79, "y":0, "z":0}, "scale":{"x":1.21, "y":0.92, "z":0.92}, "rot":{"x":180,"y":0,  "z":0}, "materialOverride":[["disc_brake", "disc_brake_FL"]]}],
    ],
    "glowMap":{
        "disc_brake_FR": {"simpleFunction":{"brakeGlow_FR":1}, "off":"disc_brake", "on":"disc_brake_glow", "materialEmissiveScaling":{"on_max":1.00}},
        "disc_brake_FL": {"simpleFunction":{"brakeGlow_FL":1}, "off":"disc_brake", "on":"disc_brake_glow", "materialEmissiveScaling":{"on_max":1.00}},
    },
