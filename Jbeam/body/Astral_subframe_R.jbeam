    ],
    "nodes": [
        ["id", "posX", "posY", "posZ"],
        {"frictionCoef":0.5},
        {"collision":true},
        {"selfCollision":false},
        {"nodeMaterial":"|NM_METAL"},
        //--Subframe--
        //running gear
        {"nodeWeight":6},
        {"group":["Astral_sub_R", "Astral_arm_R", "Astral_rod_R"]},
        ["rx1r", -0.202, 1.00, 0.115],
        ["rx1l",  0.202, 1.00, 0.115],
        ["rx2r", -0.202, 1.32, 0.115],
        ["rx2l",  0.202, 1.32, 0.115],
        //mounting points
        {"nodeWeight":3.5},
        {"group":["Astral_sub_R"]},
        ["rx3r", -0.378, 0.964, 0.369],
        ["rx3l",  0.378, 0.964, 0.369],
        ["rx4r", -0.353, 1.356, 0.375],
        ["rx4l",  0.353, 1.356, 0.375],
        //rigidifier
        {"collision":false},
        {"nodeWeight":3.5},
        {"group":[""]},
        ["rx1", 0, 1.15, 0.45],
        {"collision":true},
        {"selfCollision":true},
    ],  
    "beams": [
        ["id1:", "id2:"],
        //--Subframe--
        //running gear
        {"beamSpring":8001000,"beamDamp":180},
        {"beamDeform":105000,"beamStrength":"FLT_MAX"},
        ["rx1r", "rx2r"],
        ["rx2r", "rx2l"],
        ["rx2l", "rx1l"],
        ["rx1l", "rx1r"],
        //mounting points
        {"beamSpring":3501000,"beamDamp":150},
        {"beamDeform":75000,"beamStrength":"FLT_MAX"},
        ["rx3r", "rx3l"],
        ["rx4r", "rx4l"],
        ["rx3r", "rx4r"],
        ["rx3l", "rx4l"],
        ["rx3l", "rx1l"],
        ["rx3r", "rx1r"],
        ["rx4l", "rx2l"],
        ["rx4r", "rx2r"],
        //crossing
        {"beamSpring":8001000,"beamDamp":180},
        {"beamDeform":75000,"beamStrength":"FLT_MAX"},
        ["rx1r", "rx2l"],
        ["rx1l", "rx2r"],
        {"beamSpring":3501000,"beamDamp":150},
        ["rx4l", "rx3r"],
        ["rx4r", "rx3l"],
        ["rx4l", "rx2r"],
        ["rx2l", "rx4r"],
        ["rx3l", "rx1r"],
        ["rx1l", "rx3r"],
        ["rx2r", "rx3r"],
        ["rx1r", "rx4r"],
        ["rx2l", "rx3l"],
        ["rx4l", "rx1l"],
        //rigids
        {"beamDeform":35000,"beamStrength":"FLT_MAX"},
        ["rx2l", "rx3r"],
        ["rx2r", "rx3l"],
        ["rx1l", "rx4r"],
        ["rx1r", "rx4l"],
        //rigidifier
        {"beamSpring":4501000,"beamDamp":150},
        {"beamDeform":25000,"beamStrength":"FLT_MAX"},
        ["rx1", "rx1l"],
        ["rx1", "rx2l"],
        ["rx1", "rx1r"],
        ["rx1", "rx2r"],
        //attach to body
        {"beamSpring":3501000,"beamDamp":150},
        {"beamDeform":75000,"beamStrength":"FLT_MAX"},
        ["rx3r", "a41"],
        ["rx3r", "a37"],
        ["rx3r", "chs1r"],
        ["rx3r", "chs2r"],
        ["rx4r", "a37"],
        ["rx4r", "a41"],
        ["rx4r", "chs1r"],
        ["rx4r", "chs2r"],
        ["rx1r", "a9"],
        ["rx1r", "a10"],
        ["rx1r", "a41"],
        ["rx2r", "a38"],
        ["rx2r", "a37"],
        //*
        ["rx3l", "a42"],
        ["rx3l", "a38"],
        ["rx3l", "chs1l"],
        ["rx3l", "chs2l"],
        ["rx4l", "a38"],
        ["rx4l", "a42"],
        ["rx4l", "chs1l"],
        ["rx4l", "chs2l"],
        ["rx1l", "a10"],
        ["rx1l", "a9"],
        ["rx1l", "a42"],
        ["rx2l", "a37"],
        ["rx2l", "a38"],

        //--Limiters--
        //subframe limiter
        {"beamPrecompression":0.98, "beamType":"|SUPPORT", "beamLongBound":2},
        {"beamSpring":4001000,"beamDamp":100},
        {"beamDeform":15000,"beamStrength":80000},
        {"deformLimitExpansion":""},
        ["rx3r", "a9"],
        ["rx3r", "a41"],
        ["rx3r", "a13"],
        ["rx3r", "chs1r"],
        ["rx4r", "a37"],
        ["rx4r", "chs1r"],
        ["rx1r", "a41"],
        ["rx2r", "chs1r"],
        //*
        ["rx3l", "a10"],
        ["rx3l", "a41"],
        ["rx3l", "a14"],
        ["rx3l", "chs1l"],
        ["rx4l", "a38"],
        ["rx4l", "chs1l"],
        ["rx1l", "a42"],
        ["rx2l", "chs1l"],
        {"beamPrecompression":1},
        {"beamSpring":false,"beamDamp":false,"beamDeform":false,"beamStrength":false},
        {"beamLimitSpring":false,"beamLimitDamp":false},
        {"beamPrecompression":1},
        {"beamType":"|NORMAL", "beamLongBound":1.0, "beamShortBound":1.0}
    ],
    "triangles": [
        ["id1:", "id2:", "id3:"],
        {"groundModel":"metal"},
        {"dragCoef":0},
        {"triangleType":"NORMALTYPE"},
        //cross member
        ["rx1r", "rx2r", "rx1l"],
        ["rx1l", "rx2r", "rx2l"]
    ]
